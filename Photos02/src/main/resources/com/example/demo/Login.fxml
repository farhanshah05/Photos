<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <children>
        <Label layoutX="200.0" layoutY="50.0" prefHeight="50.0" prefWidth="200.0" text="Welcome!" textAlignment="CENTER" style="-fx-font-size: 24.0; -fx-font-family: 'Arial'; -fx-text-fill: black;" />
        <Label layoutX="150.0" layoutY="140.0" text="Username:" style="-fx-font-family: 'Arial'; -fx-font-size: 14.0;" />
        <TextField fx:id="tfUsername" layoutX="150.0" layoutY="170.0" prefHeight="40.0" prefWidth="300.0" onKeyPressed="#handleEnterKey" promptText="Username" style="-fx-font-family: 'Arial'; -fx-font-size: 14.0;" />
<!--        <TextField fx:id="tfUsername" onKeyPressed="#handleEnterKey"/>-->
        <Button fx:id="mLogIn" layoutX="250.0" layoutY="230.0" mnemonicParsing="false" onAction="#login" onMouseClicked="#login" prefHeight="40.0" prefWidth="100.0" text="Log In" style="-fx-background-color: #4CAF50; -fx-text-fill: white;" />
    </children>
</AnchorPane>
